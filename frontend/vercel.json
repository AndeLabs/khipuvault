{
  "buildCommand": "cd frontend && npm run build",
  "outputDirectory": "frontend/.next",
  "framework": "nextjs",
  "nodeVersion": "20.x",
  "build": {
    "env": {
      "NEXT_TELEMETRY_DISABLED": "1"
    }
  },
  "functions": {
    "frontend/app/**/*.tsx": {
      "runtime": "nodejs20.x"
    }
  }
}