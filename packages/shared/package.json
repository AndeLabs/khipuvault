{
  "name": "@khipu/shared",
  "version": "1.0.0",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./constants/chains": {
      "types": "./dist/constants/chains.d.ts",
      "import": "./dist/constants/chains.mjs",
      "require": "./dist/constants/chains.js"
    },
    "./config/network": {
      "types": "./dist/config/network.d.ts",
      "import": "./dist/config/network.mjs",
      "require": "./dist/config/network.js"
    },
    "./addresses": {
      "types": "./dist/addresses/index.d.ts",
      "import": "./dist/addresses/index.mjs",
      "require": "./dist/addresses/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts src/constants/chains.ts src/config/network.ts src/addresses/index.ts --format esm,cjs --dts",
    "dev": "tsup src/index.ts src/constants/chains.ts src/config/network.ts src/addresses/index.ts --format esm,cjs --dts --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "viem": "^2.21.58",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.3.3"
  }
}
